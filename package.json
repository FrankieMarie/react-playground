{
  "name": "react-playground",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "frankiemarie",
  "license": "ISC",
  "scripts": {
    "client": "npm run dev --prefix client",
    "build-client": "npm run build --prefix client",
    "dev": "dotenvx run -- ts-node-dev api/index.ts",
    "start": "dotenvx run -- node api/index.ts",
    "migrations:generate": "drizzle-kit generate:sqlite --config=drizzle.config.ts",
    "migrations:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:push": "drizzle-kit push:sqlite --config=drizzle.config.ts",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@auth/express": "^0.5.3",
    "@dotenvx/dotenvx": "^0.35.1",
    "@libsql/client": "^0.6.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.30.8",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "drizzle-kit": "^0.20.17",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
